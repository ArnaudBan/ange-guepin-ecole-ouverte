  "description": "",
  "main": "index.js",
  "keywords": ["11ty", "SASS", "SCSS", "eleventy"],
  "author": "Nikita Zanella",
  "license": "ISC",
  "scripts": {
    "watch:sass": "node-sass --watch src/sass -o src/css",
